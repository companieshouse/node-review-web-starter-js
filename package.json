{
  "name": "web-starter",
  "version": "1.0.0",
  "description": "A starter web application for Node.js, with native Javascript",
  "main": "src/server.js",
  "scripts": {
    "start": "source ./src/config/.env && gulp build && node server.js",
    "watch": "source ./src/config/.env && nodemon -e js,json,html,njk --watch ./src --config ./config/nodemon.json .src/server.js",
    "build": "gulp build",
    "test": "source ./src/config/.env && mocha --recursive test/src",
    "test:watch": "source ./src/config/.env && mocha --recursive --watch --reporter nyan test/src",
    "coverage": "nyc --check-coverage --reporter=text-summary npm run test",
    "coverage:report": "nyc --reporter=lcov --reporter=text npm run test",
    "test:coverage": "nyc npm test",
    "lint": "eslint 'src/*.js' 'src/**/*.js' 'test/*.js' 'test/**/*.js'",
    "lint:fix": "eslint 'src/*.js' 'src/**/*.js' 'test/*.js' 'test/**/*.js' --fix"
  },
  "author": "Moses Wejuli <mwejuli@companieshouse.gov.uk>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "body-parser": "1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^3.2.0",
    "govuk_frontend_toolkit": "^7.6.0",
    "moment": "2.24.0",
    "moment-timezone": "^0.5.23",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.11",
    "nunjucks": "3.1.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "chai": "^4.0.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "eslint": "5.13.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "gulp": "4.0.0",
    "gulp-clean": "0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-exec": "^2.1.3",
    "gulp-minify": "^3.1.0",
    "gulp-sass": "3.2.1",
    "gulp-shell": "^0.6.5",
    "gulp-uglify": "^3.0.2",
    "husky": "^4.3.8",
    "mocha": "^4.1.0",
    "pre-commit": "^1.2.2",
    "sinon": "6.3.5",
    "sinon-chai": "^3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "eslintConfig": ".eslintrc",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/companieshouse/node-review-web-starter-js.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/companieshouse/node-review-web-starter-js/issues"
  },
  "homepage": "https://github.com/companieshouse/node-review-web-starter-js#readme"
}
