{
  "name": "node-web-starter-js",
  "version": "1.0.0",
  "description": "A starter web application for Node.js, with native Javascript",
  "main": "src/server.js",
  "scripts": {
    "build": "gulp build",
    "lint": "eslint 'src/*.js' 'src/**/*.js' 'test/*.js' 'test/**/*.js'",
    "lint:fix": "eslint 'src/*.js' 'src/**/*.js' 'test/*.js' 'test/**/*.js' 'gulpfile.js' --fix",
    "coverage": "nyc --check-coverage --reporter=text-summary npm run test",
    "coverage:report": "nyc --reporter=lcov --reporter=text npm run test",
    "test:coverage": "nyc npm test",
    "test": "source ./src/config/.env && mocha --recursive test/src",
    "test:watch": "source ./src/config/.env && mocha --recursive --watch --reporter nyan test/src",
    "start": "source ./src/config/.env && gulp build && node src/server.js",
    "start:watch": "source ./src/config/.env && npm run build && nodemon"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "author": "Moses Wejuli <mwejuli@companieshouse.gov.uk>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "body-parser": "1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^4.5.0",
    "govuk_frontend_toolkit": "^9.0.1",
    "moment": "2.24.0",
    "moment-timezone": "^0.5.23",
    "morgan": "^1.9.0",
    "nunjucks": "3.1.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "chai": "^4.0.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "gulp": "4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^5.1.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^4.3.8",
    "mocha": "^4.1.0",
    "nodemon": "^1.14.11",
    "sass": "^1.59.2",
    "sinon": "6.3.5",
    "sinon-chai": "^3.3.0"
  },
  "eslintConfig": ".eslintrc",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/companieshouse/node-review-web-starter-js.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/companieshouse/node-review-web-starter-js/issues"
  },
  "nodemonConfig": {
    "watch": [
      "./src",
      "./assets/src"
    ],
    "ext": "js,json,njk,scss,env",
    "legacyWatch": true,
    "ignore": [
      "**/*.test.ts"
    ],
    "exec": "source ./src/config/.env && npm run build && node ./src/server.js"
  },
  "homepage": "https://github.com/companieshouse/node-review-web-starter-js#readme"
}
